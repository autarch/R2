<h2>Members</h2>

<ul>
% while ( my ( $person, $membership ) = $members->next() ) {
  <li><a href="<% $person->contact()->uri() %>"
         title="View <% $person->full_name() %>"
         ><% $person->full_name() %></a>
%   if ( my $pos = $membership->position() ) {
      - <% $pos %>
%   }
  </li>
% }
</ul> 

<%args>
$contact
</%args>

<%init>
my $members = $contact->members();
</%init>
